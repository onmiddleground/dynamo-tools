{
  "name": "@onmiddleground/dynamo-tools",
  "version": "1.0.4",
  "description": "Useful Dynamo Tools for development and testing",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.json",
    "dist/**/*.d.ts"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "clean": "node ./node_modules/rimraf/bin.js dist",
    "build": "npm run clean && node ./node_modules/typescript/bin/tsc",
    "watch": "npm run clean && node ./node_modules/typescript/bin/tsc --watch",
    "test": "mocha -r ts-node/register src/**/*-spec.ts",
    "release:publish": "npm run build && npm version patch --force && npm publish",
    "local:publish": "npm run build && npm unpublish --force && npm publish"
  },
  "author": "Gary Waddell",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.10.5",
    "pino": "^8.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.15.15",
    "@types/sinon": "^9.0.4",
    "aws-sdk": "^2.726.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.11",
    "ts-node": "^4.1.0",
    "tsc-watch": "^1.1.35",
    "tslib": "^1.13.0",
    "tslint": "^5.12.1",
    "typescript": "^3.9.5"
  },
  "repository": {
    "directory": "dist/"
  }
}
